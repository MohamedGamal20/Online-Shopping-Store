@model IEnumerable<Online_Shopping_Store.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Browse category</h2>
<p>
    Select from @Model.Count()
    genres:
</p>
<ul>
    @foreach (var category in Model)
    {
        <li>@Html.ActionLink(category.Name, "Brose", new { category = category.Name })</li>
    }
</ul>
